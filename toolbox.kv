#:import toolbox toolbox
<ToolBox@BoxLayout>:
	orientation:'vertical'
	id:tool_box
	tool_box:tool_box
	area:area
	rooms:rooms
	bedroom:bedroom
	budget:budget
	generatebutton:generatebutton
	Label:
		text:'Budget(lakhs)'
		size_hint:None,None
		size:100,34
	Spinner:
		id:budget
		text:'select'
		values:'10','15','20','30','40'
		size_hint:None,None
		size:90,34
		pos_hint:{'center_x':.5,'center_y':.5}
		on_text:self.parent.set_area()
	Label:
		text:'Area(sq.ft)'
		size_hint:None,None
		size:100,34
	Spinner:
		id:area
		text:'600'
		values:'600','650','700'
		size_hint:None,None
		size:90,34
		pos_hint:{'center_x':.5,'center_y':.5}
		disabled:True
	Label:
		text:'Rooms'
		size_hint:None,None
		size:100,34
	Spinner:
		id:rooms
		text:'5'
		values:'5','6','7'
		size_hint:None,None
		size:90,34
		pos_hint:{'center_x':.5,'center_y':.5}
		disabled:True
		
	Label:
		text:'Bedroom'
		size_hint:None,None
		size:100,34
	Spinner:
		id:bedroom
		text:'2'
		values:'1','2'
		size_hint:None,None
		size:90,34
		pos_hint:{'center_x':.5,'center_y':.5}
		disabled:True
	Label:
		text:'Bathroom'
		size_hint:None,None
		size:100,34
	Spinner:
		id:bathroom
		text:'1'
		values:'1','2'
		size_hint:None,None
		size:90,34
		pos_hint:{'center_x':.5,'center_y':.5}
		disabled:True
	Label:
		text:'Hall'
		size_hint:None,None
		size:100,34
	Spinner:
		id:hall
		text:'1'
		values:'1','2'
		size_hint:None,None
		size:90,34
		pos_hint:{'center_x':.5,'center_y':.5}
		disabled:True
	Label:
		text:'git 2'
		
	Button:
		id:generatebutton
		area:area
		text:'Generate'
		size_hint:None,None
		size:90,44
		pos_hint:{'center_x':.5,'center_y':.5}
		background_color: (.83,.4,.82,1)
		on_press:self.parent.calculate()
	Label:
		text:''